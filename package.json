{
  "name": "make-it-app",
  "version": "1.0.0",
  "description": "MakeIt app offers useful and user-friendly features to organize your tasks and optimize your productivity.",
  "scripts": {
    "start": "npm run dev-server",
    "dev-server": "webpack-dev-server --progress --config ./config/webpack.config.js",
    "dev-server-prod": "webpack-dev-server --env.production --progress --config ./config/webpack.config.js",
    "build": "rimraf build && webpack --progress --display-modules --config ./config/webpack.config.js",
    "build-prod": "rimraf build && webpack --env.production --display-modules --config ./config/webpack.config.js",
    "lint": "eslint .",
    "serve": "node ./src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "npm run build-prod"
  },
  "dependencies": {
    "angular": "1.8.0",
    "angular-animate": "1.6.4",
    "angular-fontawesome": "1.0.0",
    "angular-local-storage": "0.5.2",
    "angular-sanitize": "1.6.4",
    "angular-toastr": "2.1.1",
    "angular-ui-bootstrap": "2.5.0",
    "angular-ui-router": "1.0.0-rc.1",
    "awesome-bootstrap-checkbox": "1.0.0-alpha.5",
    "bootstrap": "3.4.1",
    "bootswatch": "3.3.7",
    "express": "4.15.2",
    "fbemitter": "2.1.1",
    "font-awesome": "4.7.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-plugin-angularjs-annotate": "0.7.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.27.3",
    "eslint": "3.17.1",
    "eslint-loader": "1.6.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "less": "2.7.2",
    "less-loader": "3.0.0",
    "postcss-loader": "1.3.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "style-loader": "0.13.2",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
